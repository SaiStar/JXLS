<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
    <worksheet sheetIdx="0">
        <section startRow="0" endRow="7"></section>
        <loop startRow="8" endRow="8" items="employees" var="employee" varType="com.Employee">
<!--            ?????-->
            <section startRow="8" endRow="8">
                <mapping cell="A8">employee.name</mapping>
                <mapping cell="B8">employee.age</mapping>
                <mapping cell="D8">employee.payment</mapping>
                <mapping cell="E8">employee.bonus</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0">Employee Payment Totals:</cellcheck>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>